{
  "name": "server",
  "version": "0.1.0",
  "scripts": {
    "start": "node -r dotenv/config dist/index",
    "dev": "node --watch -r dotenv/config dist/index",
    "build": "swc src -d dist --delete-dir-on-start",
    "build:dev": "swc src -w -d dist"
  },
  "dependencies": {
    "@swc/cli": "^0.1.57",
