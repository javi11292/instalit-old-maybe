    "connect-mongodb-session": "^3.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "mongodb": "^4.11.0"
  },
  "devDependencies": {
    "@types/connect-mongodb-session": "^2.4.4",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "chokidar": "^3.5.3",
    "concurrently": "^7.4.0",
    "typescript": "4.8.4"
  }
}
