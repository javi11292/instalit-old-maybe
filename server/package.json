{
  "name": "server",
  "version": "0.1.0",
  "scripts": {
    "start": "node -r dotenv/config dist/index",
    "dev": "node --watch -r dotenv/config dist/index",
    "build": "swc src -d dist --delete-dir-on-start",
    "build:dev": "swc src -w -d dist"
  },
  "dependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "chokidar": "^3.5.3"
  }
}
