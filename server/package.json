{
  "name": "server",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run docker -- up --build -d",
    "dev": "npm run docker -- -f docker-compose.development.yml up --build",
    "build": "swc src -d dist --delete-dir-on-start",
    "build:dev": "swc src -w -d dist",
    "docker": "docker-compose -f docker-compose.yml"
  },
  "dependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.5",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "18.11.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "chokidar": "^3.5.3",
    "concurrently": "^7.4.0",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "typescript": "4.8.4"
  }
}
